#include <stdio.h>

int main() {
    int age;

    // Prompt user to enter their age
    printf("Enter your age: ");
    scanf("%d", &age);

    // Check voting eligibility using switch-case
    switch (age) {
        case 18:
            printf("You are eligible to vote for the first time!\n");
            break;
        case 19:
        case 20:
        case 21:
        case 22:
        case 23:
        case 24:
        case 25:
            printf("You can vote!\n");
            break;
        default:
            printf("You are not eligible to vote yet.\n");
            break;
    }

    return 0;
}
