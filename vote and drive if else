// program to check whether user can vote & drive or not 
// age is taken input from user 

#include <stdio.h>

int main() {

    int age ;
    printf("enter your age : ");
    scanf("%d" , &age) ;
    
    if(age < 18)
       printf(" you are not eligible to vote or drive .");
    else 
      printf (" you are eligible to vote as well as drive.");

    return 0;

}
